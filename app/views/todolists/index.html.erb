<!--投稿一覧画面-->

<h1>Books</h1>
<teble>
  <thead>
    <tr>
      <th> </th>
      <th>Title</th>
      <th>Opinion</th>
    </tr>
  </thead>
  <tbody>
    <%= @books.echo do |book| %>
    <tr>
      <td><!--プロフ画像をいれる、からむuser_idが足りない--></td>
      <td><%= book.title  %></td>
      <td><%= book.body  %></td>
    </tr>
    <% end %>
  </tbody>
</teble>